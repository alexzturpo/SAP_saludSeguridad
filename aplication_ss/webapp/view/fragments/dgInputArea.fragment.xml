<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" >
	<SelectDialog title="Area"
		items="{ path: 'myParam>/ZSYSO_AREASet'}"
		search="dgSearchArea"
		confirm="dgGetCloseArea"
		cancel="dgGetCloseArea">
		<StandardListItem
			icon="sap-icon://building"
			iconDensityAware="false"
			iconInset="false"
			title="{myParam>ZDESCRIPCION}"
			description="{myParam>ZAREA}" />
	</SelectDialog>
</core:FragmentDefinition> 