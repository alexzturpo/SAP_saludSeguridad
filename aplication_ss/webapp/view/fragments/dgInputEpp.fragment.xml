<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" >
	<SelectDialog title="Material"
		items="{ path: 'myParam>/dataTab_Material'}"
		search="dgSearchEpp"
		confirm=".dgGetCloseEpp"
		cancel=".dgGetCloseEpp">
		<StandardListItem
			icon="sap-icon://product"
			iconDensityAware="false"
			iconInset="false"
			title="{myParam>nombre}"
			description="{myParam>id}"
            info="Stock : {myParam>stock}" />
	</SelectDialog>
</core:FragmentDefinition> 